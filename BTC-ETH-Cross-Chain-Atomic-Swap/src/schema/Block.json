{
  "$id": "https://dev.liquality.com/schema/block.json",
  "title": "Block",
  "description": "Blockchain block",
  "type": "object",
  "required": [ "number", "hash", "timestamp", "difficulty", "size", "parentHash", "nonce" ],
  "properties": {
    "hash": {
      "type": "string",
      "title": "Hash",
      "pattern": "^([A-Fa-f0-9]{64})$"
    },
    "number": {
      "type": "number",
      "title": "Number",
      "minimum": 0
    },
    "timestamp": {
      "type": "number",
      "title": "Timestamp",
      "minimum": 0
    },
    "difficulty": {
      "type": "string",
      "title": "Difficulty",
      "minLength": 1
    },
    "size": {
      "type": "number",
      "title": "Size",
      "minimum": 0
    },
    "parentHash": {
      "type": "string",
      "title": "Parent Hash",
      "pattern": "^([A-Fa-f0-9]{64})$"
    },
    "nonce": {
      "type": "number",
      "title": "Nonce",
      "minimum": 0
    },
    "_raw": {
      "type": "object"
    }
  }
}
